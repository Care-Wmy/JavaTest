[
  {
    "description": "模拟一个没有参数的get请求",
    "request":{
      "uri": "/getdemo",
      "method": "get"
    },
    "response": {
      "text": "这是一个没有参数的get请求",
      "headers":{"Content-Type":"text/html;charset=gbk"}
    }
  },

  {
    "description": "模拟一个有参数的get请求",
    "request": {
      "uri": "/getwithparam",
      "method": "get",
      "queries":{"name": "昆旗","age": "21"}
    },
    "response": {
      "text": "昆旗来了哈哈哈哈哈",
      "headers":{"Content-Type":"text/html;charset=gbk"}
    }
  }
]